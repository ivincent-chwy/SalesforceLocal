<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Prevent_Greater_Than_100_Service</fullName>
    <active>true</active>
    <errorConditionFormula>IF(ServiceAllocated__c &gt; 1, TRUE, FALSE)</errorConditionFormula>
    <errorMessage>Service allocations exceed 100%</errorMessage>
</ValidationRule>
